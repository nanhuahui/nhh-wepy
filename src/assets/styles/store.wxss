/*通知栏*/

.notice-box {
  display: flex;
  background: white;
}

.notice {
  padding-left: 20rpx;
  width: 412rpx;
  height: 30px;
  display: inline-block;
  line-height: 2;
}

.notice image.notice_img {
  width: 34rpx;
  height: 31rpx;
  vertical-align: sub;
  display: inline-block;
}

.notice text {
  font-size: 28rpx;
  font-weight: bold;
  color: #D2A475;
  display: inline-block;
  margin-left: 10rpx;
  position: relative;
}

.notice text:before {
  content: '';
  height: 33rpx;
  width: 1px;
  position: absolute;
  top: 13rpx;
  left: -9rpx;
  background: #eee;
}


